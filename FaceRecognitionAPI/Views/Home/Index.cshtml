    <input type="button" title="SubmitPicture" name="http://www.veteranstoday.com/wp-content/uploads/2013/07/winston-churchill.jpg" value="Picture4" width="25" id="callFace4" />
    <input type="button" title="SubmitPicture" name="https://commonsblog.files.wordpress.com/2009/12/sitting_bull.jpg" value="Picture5" width="25" id="callFace5" />
